local screen = script.Parent.Screen
local Lock = script.Parent.Screen.Main.Lock
local Main = script.Parent.Screen.Main
local Console = script.Parent.Screen.Main.Console

local Settings = require(script.Parent.Settings)
local Debris = game:GetService("Debris")
local Template = Main.Console.BG.Template
local BG = Main.Console.BG

Main:FindFirstChild("Lock"):FindFirstChild("Unlock").MouseButton1Click:Connect(function()
	Lock.Visible = false
	Console.Visible = true
	
end)

for _, v in pairs(Settings) do
	local button = Template:Clone()
	button.Name = v.Name
	button.Text = 'Play "' .. v.Name .. '"'
	button.Parent = BG
	button.Visible = true

	button.MouseButton1Click:Connect(function()
		local sound = Instance.new("Sound")
		sound.SoundId = v.SoundID
		sound.Volume = v.Volume
		sound.Pitch = v.Pitch
		sound.Parent = workspace
		sound:Play()

		Debris:AddItem(sound, 2)
	end)
end
